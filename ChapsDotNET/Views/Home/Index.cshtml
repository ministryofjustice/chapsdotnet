@using ChapsDotNET.Frontend.Components.Breadcrumbs
@using ChapsDotNET.Frontend.Components.Heading
@{
    Layout = ViewData["LayoutName"] as string;
}
.
@{
    ViewData["Title"] = "Home Page";

    // Breadcrumbs
    List<BreadcrumbModel> breadcrumbs = [
        new BreadcrumbModel { Label = "Home", Url = Url.Action("Index", "Home", new {area =""})!},
    ];
    var breadcrumbsModel = new BreadcrumbsModel(breadcrumbs);

    // Heading
    var headingModel = new HeadingModel { Title = "Home Page" };
}

<partial name="~/Frontend/Components/Breadcrumbs/_Breadcrumbs.cshtml" model="breadcrumbsModel" />
<partial name="~/Frontend/Components/Heading/_Heading.cshtml" model="headingModel" />
