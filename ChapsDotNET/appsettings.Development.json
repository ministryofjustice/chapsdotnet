{
  "ReverseProxy": {
    "Routes": {
      "Old_chaps": {
        "ClusterId": "framework_481_Cluster",
        "AuthorizationPolicy": "bypassOpenId",
        "Match": {
          "Path": "{**any}"
        },
        "Order": 2
      },
      "StaticFiles": {
        "ClusterId": "StaticFilesCluster",
        "Match": {
          "Path": "/content/{**any}"
        },
        "Order": 1
      }
    },  
    "Clusters": {
      "framework_481_Cluster": {
        "Destinations": {
          "framework481_app": {
            "Address": "https://localhost:44300/"
          }
        }
      },
      "StaticFilesCluster": {
        "Destinations": {
          "StaticFilesApp": {
            "Address": "https://localhost:44300/content/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Yarp.ReverseProxy": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CLIENT_ID": "dummy-client-id-for-dev"
}
